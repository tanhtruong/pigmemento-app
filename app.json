{
  "expo": {
    "name": "pigmemento-app",
    "slug": "pigmemento-app",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [["expo-secure-store"]],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.pigmemento.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiBaseUrl": "https://api.pigmemento.app",
      "eas": {
        "projectId": "91cc7e68-135a-4239-a94d-22a46a6f00ad"
      }
    }
  }
}
