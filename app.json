{
  "expo": {
    "name": "pigmemento",
    "slug": "pigmemento",
    "owner": "tuandrengen",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/pigmemento_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/pigmemento_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [["expo-secure-store"]],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.pigmemento.mobile",
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiBaseUrl": "http://localhost:5197",
      "eas": {
        "projectId": "feaea300-68de-471d-9b9e-f0a2ad3fe071"
      }
    }
  }
}
